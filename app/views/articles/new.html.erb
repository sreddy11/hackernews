<h1> Submit </h1>

<% if session[:user_id] == nil %>
  <p> Please login first </p>
<% else %>
  <%= form_for @article do |f| %>
    <p>
      <%= f.label :title %>
      <%= f.text_field :title %>
    </p>
    <p>
      <%= f.label :url %>
      <%= f.text_field :url %>
    </p>
    <p>
      <%= f.submit %>
    </p>
  <% end %>
<% end %>
