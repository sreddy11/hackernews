<% @articles.each do |article| %>
  <h2> <%= link_to article.title, article.url %> <font size="1"> <%="( #{article.domain} )" %> </font></h2>

  <p>
  Posted by <%= link_to "#{article.user.user_name}", user_path(article.user) %> | about 
  <%= time_ago_in_words article.created_at %> ago |
  <%= link_to "0 Comments", article_path(article) %>
  </p>  
<% end %> 
 
<%= will_paginate @articles %>
