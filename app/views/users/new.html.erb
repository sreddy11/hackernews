<h1> Register </h1>


<ul>
    <% @user.errors.full_messages.each do |error| %>
      <li><%= error %></li>
    <% end %>
</ul>

<%= form_for @user do |f| %>
  <p>
    <%= f.label :user_name, "Username" %>
    <%= f.text_field :user_name %>
  </p>
  <p>
    <%= f.label :password %>
    <%= f.password_field(:password) %>
    </p>  
  <p>
    <%= f.label :password_confirmation, "Confirm Password" %>
    <%= f.password_field(:password_confirmation) %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
